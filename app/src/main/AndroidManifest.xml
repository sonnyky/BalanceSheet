<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tinker.graphit" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_graphit"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tinker.graphit.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tinker.graphit.GraphActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.tinker.graphit.WebViewActivity"
            android:theme="@android:style/Theme.NoTitleBar" />

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="5e7b2d3b3a45023239643b1dcd1e1cb3d5650563" />
    </application>

</manifest>
